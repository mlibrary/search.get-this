{
  "name": "get-this",
  "version": "1.0.0",
  "description": "Implements date picker for booking a media item",
  "main": "index.js",
  "scripts": {
    "build": "FILE_TYPE=css npm run build:type && FILE_TYPE=js npm run build:type",
    "build:type": "esbuild \"$FILE_TYPE/*.$FILE_TYPE\" --bundle --minify --outdir=public/bundles $*",
    "watch:css": "FILE_TYPE=css npm run build:type -- --watch",
    "watch:js": "FILE_TYPE=js npm run build:type -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlibrary/get-this.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mlibrary/get-this/issues"
  },
  "homepage": "https://github.com/mlibrary/get-this#readme",
  "devDependencies": {
    "esbuild": "0.25.11"
  }
}
